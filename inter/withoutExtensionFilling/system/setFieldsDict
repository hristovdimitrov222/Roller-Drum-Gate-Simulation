FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Code template and library provided by Thorenz, C. (2024), DOI: 10.3929/ethz-b-000675949

defaultFieldValues
(
    volScalarFieldValue alpha.water 1//only water in the domain
);

regions
(
    boxToCell
    {
        box (0 0 0.42) (0.8 0.5 0.7);
        fieldValues
        (
            volScalarFieldValue alpha.water 0//we set our block of air into the domain
        );
    }
	
	    boxToCell
    {
        box (0.8 0 0.273) (2.5 0.5 0.7); //0.326
        fieldValues
        (
            volScalarFieldValue alpha.water 0//we set our block of air into the domain
        );
    }
);


