FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * /

scale 1;

xMin 0.19;
xMax 2.27;
yMin 0.3;
yMax 0.501;
zMin -0.01;
zMax 0.649;

vertices
(
    ($xMin $yMin $zMin)
    ($xMax $yMin $zMin)
    ($xMax $yMax $zMin)
    ($xMin $yMax $zMin)
    ($xMin $yMin $zMax)
    ($xMax $yMin $zMax)
    ($xMax $yMax $zMax)
    ($xMin $yMax $zMax)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) (135 13 43) simpleGrading (1 1 1)
);

edges
(
);

boundary
(
    boundary
    {
        type patch;
        faces
        (
            (7 3 0 4)
            (6 5 1 2)
            (0 1 5 4)
            (3 7 6 2)
            (3 2 1 0)
            (4 5 6 7)
        );
    }

);


