FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      surfaceFeatureExtractDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * /


atmosphere.stl
{

    extractionMethod extractFromSurface;

    includedAngle 150;

    subsetFeatures
    {

        nonManifoldEdges false;

        openEdges true;
    }

    writeObj true;
}

